    "import pandas as pd\n",
    "from sklearn.linear_model import LinearRegression\n",
    "\n",
    "# Sample Data\n",
    "hours = [[2], [3], [4], [5]]\n",
    "marks = [50, 60, 65, 75]\n",
    "\n",
    "# Create model\n",
    "model = LinearRegression()\n",
    "model.fit(hours, marks)\n",
    "\n",
    "# Predict\n",
    "print(\"Predicted marks if 8 hours studied:\", model.predict([[8]]))"
   ]
  }
